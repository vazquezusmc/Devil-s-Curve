  3.7477067591295557`*^9}},ExpressionUUID->"91a86b58-b939-4a53-adca-\
9d04e9e0becf"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"yvalues", " ", "=", " ", 
   RowBox[{"y", "/.", 
    RowBox[{"Solve", "[", 
     RowBox[{
      RowBox[{
       RowBox[{
        SuperscriptBox["y", "4"], "-", 
        RowBox[{"4", 
         SuperscriptBox["y", "2"]}]}], "\[Equal]", 
       RowBox[{
        SuperscriptBox["x", "4"], "-", 
        RowBox[{"9", 
         SuperscriptBox["x", "2"]}]}]}], "/.", 
      RowBox[{"x", "\[Rule]", " ", "0.5"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"tanvalues", " ", "=", " ", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"m", "/.", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"x", "\[Rule]", "0.5"}], ",", 
           RowBox[{
            RowBox[{"y", "[", "x", "]"}], "\[Rule]", 
            RowBox[{"yvalues", "[", 
             RowBox[{"[", "i", "]"}], "]"}]}]}], "}"}]}], ")"}], 
       RowBox[{"(", 
        RowBox[{"x", "-", "0.5"}], ")"}]}], "+", 
      RowBox[{"yvalues", "[", 
       RowBox[{"[", "i", "]"}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "4"}], "}"}]}], "]"}]}], ";"}]}], "Input",
