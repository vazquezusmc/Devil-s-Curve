Cell[CellGroupData[{

Cell["Implicit differentiation", "Subsection",
 CellChangeTimes->{{3.747705942491576*^9, 
  3.747705974606791*^9}},ExpressionUUID->"3d1fa58f-35f3-4f2a-b57b-\
b2181abfa504"],

Cell[TextData[{
 "By default Mathematica treats variables as independent. So for example ",
 StyleBox["D[y,x]",
  FontWeight->"Bold"],
 " produces ",
 StyleBox["0",
  FontWeight->"Bold"],
 ". Entering ",
 StyleBox["y[x]",
  FontWeight->"Bold"],
 " in place of ",
 StyleBox["y",
  FontWeight->"Bold"],
 " however tells Mathematica to treat y as a function of x. This enables \
implicit differentiation. \n\nWe will use the devil\[CloseCurlyQuote]s curve \
as our example."
}], "Text",
 CellChangeTimes->{{3.747706187233314*^9, 
  3.7477064001216784`*^9}},ExpressionUUID->"f27935cd-5580-4af3-938c-\
1e2397a51ddc"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"equa", " ", "=", " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SuperscriptBox[
        RowBox[{"y", "[", "x", "]"}], "4"], "-", 
       RowBox[{"4", " ", 
        SuperscriptBox[
         RowBox[{"y", "[", "x", "]"}], "2"]}]}], "\[Equal]", " ", 
      RowBox[{
       SuperscriptBox["x", "4"], "-", 
       RowBox[{"9", " ", 
        SuperscriptBox["x", "2"]}]}]}], ")"}]}], ";"}], 
  "\[IndentingNewLine]"}]], "Input",
 CellChangeTimes->{{3.747706404269422*^9, 3.747706484391967*^9}},
 CellLabel->"In[4]:=",ExpressionUUID->"de2a6386-05d7-4dc6-9788-6ba6f613abb2"],

Cell["\<\
We will use the Solve[] and D[] functions to complete the implicit \
differentiation.\
\>", "Text",
 CellChangeTimes->{{3.747706503423031*^9, 
  3.7477065569088163`*^9}},ExpressionUUID->"4ce7bd00-488a-44d1-a384-\
8ff146f75f62"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"m", " ", "=", " ", 
  RowBox[{"Solve", "[", 
   RowBox[{
    RowBox[{"D", "[", 
     RowBox[{"equa", ",", "x"}], "]"}], ",", 
    RowBox[{
     RowBox[{"y", "'"}], "[", "x", "]"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.7477065610662613`*^9, 3.747706582436343*^9}},
 CellLabel->"In[5]:=",ExpressionUUID->"5fdd0160-c2ed-4143-866b-4e07c4680e9a"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{
     SuperscriptBox["y", "\[Prime]",
      MultilineFunction->None], "[", "x", "]"}], "\[Rule]", 
    FractionBox[
     RowBox[{
      RowBox[{
       RowBox[{"-", "9"}], " ", "x"}], "+", 
      RowBox[{"2", " ", 
       SuperscriptBox["x", "3"]}]}], 
     RowBox[{"2", " ", 
      RowBox[{"y", "[", "x", "]"}], " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", "2"}], "+", 
        SuperscriptBox[
         RowBox[{"y", "[", "x", "]"}], "2"]}], ")"}]}]]}], "}"}], 
  "}"}]], "Output",
 CellChangeTimes->{{3.747706583809327*^9, 3.7477066080388517`*^9}, 
   3.7491284930658484`*^9},
 CellLabel->"Out[5]=",ExpressionUUID->"2811e192-041a-48c5-a664-f88d356e2291"]
}, Open  ]]
}, Open  ]],

